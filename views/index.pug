extends layout

block content
  h1= title
  p Welcome to Srikanth Reddy's #{title}
  button(type='button' class='btn btn-primary' onclick="moreClaps()") Press me 
  div(id="target") Clap for me
  script.
   clap_count=0;
   function moreClaps() {
     clap_count=clap_count+2;
     document.getElementById("target").innerHTML = "Clap " +clap_count+ " times";
   }
  a(href="http://localhost:5000/dogs") Dogs Link 
