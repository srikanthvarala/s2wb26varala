extends layout

block content
  h1= title
  - var images_names = ["dog.jpg","dog1.jpg","dog2.jpg","dog3.jpg","dog4.jpg"]
    - for(let i=0;i<3;i++){
        - let random = Math.floor(Math.random()*5);
            img(src=`/images/${images_names[random]}` alt="images" height="300" width="300")
    -}